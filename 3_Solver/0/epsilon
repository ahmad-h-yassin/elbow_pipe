FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 79.1015625;// U = 5 m/s ,T_u = 5% , Eddy viscosity ratio = 10

boundaryField
{

    Inlet
    {
        type            fixedValue;
        value           uniform 79.1015625;
    }
    
    Outlet
    {
        type            inletOutlet;
        inletValue      uniform 79.1015625;
    }

    Wall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    Per_1
    {
        type            cyclicAMI;
    }

    Per_2
    {
        type            cyclicAMI;
    }

}


// ************************************************************************* //
